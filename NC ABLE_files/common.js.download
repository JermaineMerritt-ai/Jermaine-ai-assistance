function getParameter(queryString1,parameterName){var queryString=new String(queryString1);parameterName=parameterName+"=";if(queryString.length>0){begin=queryString.indexOf(parameterName);if(begin!=-1){begin+=parameterName.length;end=queryString.indexOf("&",begin);if(end==-1){end=queryString.length}return unescape(queryString.substring(begin,end))}return"null"}return""}function hidediv(id){if(document.getElementById){document.getElementById(id).style.display="none"}else{if(document.layers){document.id.display="none"}else{document.all.id.style.display="none"}}}function showdiv(id){if(document.getElementById){document.getElementById(id).style.display="block"}else{if(document.layers){document.id.display="block"}else{document.all.id.style.display="block"}}}function capitalizeFirstLetter(ctrl){var val=ctrl.value;if(val!=null&&val.length>0){if(val.length==1){ctrl.value=val.toUpperCase()}else{ctrl.value=val.substring(0,1).toUpperCase()+val.substring(1)}}}function upperAOName(f){if(f&&f["participantInfo.firstName"]&&f["participantInfo.firstName"].type=="text"){capitalizeFirstLetter(f["participantInfo.firstName"]);capitalizeFirstLetter(f["participantInfo.initial"]);capitalizeFirstLetter(f["participantInfo.lastName"])}}function upperSOName(f){if(f&&f["successorInfo.firstName"]&&f["successorInfo.firstName"].type=="text"){capitalizeFirstLetter(f["successorInfo.firstName"]);capitalizeFirstLetter(f["successorInfo.initial"]);capitalizeFirstLetter(f["successorInfo.lastName"])}}function upperBeneName(f){if(f&&f["beneficiaryInfo.fname"]&&f["beneficiaryInfo.fname"].type=="text"){capitalizeFirstLetter(f["beneficiaryInfo.fname"]);capitalizeFirstLetter(f["beneficiaryInfo.initial"]);capitalizeFirstLetter(f["beneficiaryInfo.lname"])}}function sanitizeForm(){$("input[type=hidden]").each(function(i){if($(this)&&$(this).val().indexOf("|")>-1){$(this).val($(this).val().replace(/\|/g,""))}});$("input.disableCCP").bind("cut copy paste",function(e){e.preventDefault()})};